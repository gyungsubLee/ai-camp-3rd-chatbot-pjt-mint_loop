# OpenAI API Key (Required)
OPENAI_API_KEY=sk-your-openai-api-key

# Google Gemini API Key
GEMINI_API_KEY=your-gemini-api-key

# Gemini Model 설정
GEMINI_TEXT_MODEL=gemini-2.5-flash
GEMINI_IMAGE_MODEL=imagen-3.0-generate-002
IMAGE_PROVIDER=gemini

# LLM Provider 설정 (Recommendation Agent용)
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini

# Vertex AI 설정
VERTEX_CREDENTIALS_FILE=../vertex-app-key.json
VERTEX_PROJECT_ID=your-gcp-project-id
VERTEX_LOCATION=us-central1

# Google Map API Key
GOOGLE_MAP_API_KEY=your-google-map-api-key

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# 이미지 생성 설정
OPENAI_IMAGE_MODEL=dall-e-3
IMAGE_DEFAULT_SIZE=1024x1024
IMAGE_DEFAULT_QUALITY=standard
IMAGE_DEFAULT_STYLE=vivid

# 검색 API
TAVILY_API_KEY=your-tavily-api-key

# MCP 서버 설정
SEARCH_MCP_PORT=8050
IMAGE_MCP_PORT=8051

# A2A 프로토콜 사용시 (옵션)
A2A_AGENT_NAME=ImageGenerationAgent
A2A_AGENT_PORT=8080
A2A_AGENT_HOST=0.0.0.0

# 디버그 모드
DEBUG=true
LOG_LEVEL=INFO

# 성능 설정
MAX_KEYWORDS=5
SEARCH_MAX_RESULTS=3
IMAGE_TIMEOUT=60
